/**index.wxss**/
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_nav {
  display: flex;
}
.index_nav image {
  padding: 20rpx 0 10rpx 20rpx;
  flex-wrap: wrap;
  width: 160rpx;
  height: 160rpx;
}
.index_floor .floor_group .floor_title {
  margin: 20rpx 0 10rpx 5rpx;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
  height: 100%;
}
.index_floor .floor_group .floor_list {
  overflow: hidden;
}
.index_floor .floor_group .floor_list navigator {
  float: left;
  width: 31.5%;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) image {
  height: 192rpx;
}
.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
